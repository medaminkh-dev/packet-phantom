      "scheduler_behavior": "hybrid"
    },
    "metadata": {
      "target_os": "IOS",
      "version": "15.x",
      "confidence": 0.85,
      "probe_timestamp": "2024-01-15T10:30:00Z",
      "environment": "physical",
      "vendor": "Cisco",
      "family": "IOS",
      "device_type": "router"
    },
    "behavioral": {
      "congestion": {
        "algorithm": "unknown",
        "window_scaling": false
      },
      "ip": {
        "ttl": 255,
        "df_flag": false,
        "ip_id_pattern": "sequential"
      },
      "hardware": {
        "virtualization": false,
        "type": "physical"
      },
      "quirks": ["Pad"]
    }
  },
  "validation_rules": {
    "ttl_range": {"min": 1, "max": 255},
    "window_size_range": {"min": 1, "max": 65535},
    "confidence_range": {"min": 0.0, "max": 1.0}
  }
}
